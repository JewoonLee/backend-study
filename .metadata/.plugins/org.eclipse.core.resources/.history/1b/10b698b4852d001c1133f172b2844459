package 예제문제;

import java.util.Scanner;
public class javapro {
	public static void main(String[] args) {
		int num;
		Scanner scanner=new Scanner(System.in);
		System.out.print("점수 입력");
		num=scanner.nextInt();
		if(num>=90) {
			System.out.print("A");
		}
		else if(num>=80) {
			System.out.print("B");
		}
		else if(num>=70) {
			System.out.print("C");
		}
		else {
			System.out.print("F");
		}
	}}

		