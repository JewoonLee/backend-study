package 예제문제;

import java.util.Scanner;
public class javapro {
	public static void main(String[] args) {
		int num, sum=0;
		Scanner scanner=new Scanner(System.in);
		for(int i=0; i<10;i++) {
			System.out.println("더하기 할 값을 입력:");
			num=scanner.nextInt();
			sum+=num;
			}
		System.out.println("입력한 값의 더하기는 "+sum+ "입니다");
		}
	}
	