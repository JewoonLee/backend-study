package 예제문제;

import java.util.Scanner;
public class javapro {
	public static void main(String[] args) {
		int num;
		Scanner scanner=new Scanner(System.in);
		System.out.print("수 입력");
		num=scanner.nextInt();
		if(num%3==0) {
			System.out.print("3의 배수");
		}
		else {
			System.out.print("3의 배수 아님");
			
		}
		
	}}


		