package 공부하자;
import java.util.Scanner;

public class javagobu {
	static int[] ar() {
		int Array[]=new int[5];
		for(int i=0;i<Array.length;i++) {
			Array[i]=i;
		}
		return Array;
	}

	public static void main(String[] args) {
		int array[];
		array=ar();
		for(int i=0;i<array.length;i++) {
			System.out.print(array[i]);
		}
		
	}
}





