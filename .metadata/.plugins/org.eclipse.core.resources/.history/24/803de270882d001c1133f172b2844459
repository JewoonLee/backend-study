package 예제문제;
import java.util.Scanner;
public class javapro {
	public static void main(String[] args) {
		Scanner scanner=new Scanner(System.in);
		int price;
		String menu=scanner.next();
		switch (menu){
		case "카푸치노":
		case "에스프레소":
		case "카페라떼":
			price=3500;
			break;
		case "아아":
		case "레몬에이드":
			price=2500;
			break;
		default:
			System.out.print("그런 메뉴 없습니다.");
			price=0;
		}
		if(price!=0) {
			System.out.print("가격은 "+price+"입니다");
		}
		else {
			System.out.print("다시 골라주세요:");
		}
		}
	}