package 공부하자;
import java.util.Scanner;

public class javagobu {

	public static void main(String[] args) {
		Scanner n=new Scanner(System.in);
		String order=n.next();
		int price;
		switch(order) {
			case "아메리카노":
			case "카푸치노":
				price=3500;
				break;
			case "아이스":
			case "차":
				price=2000;
				break;
		}
		if(price!=0) {
			System.out.println("가격은 "+price+"입니다.");
		}
		else {
			System.out.println("그런 메뉴는 없습니다.");
			
		}
	}
}
