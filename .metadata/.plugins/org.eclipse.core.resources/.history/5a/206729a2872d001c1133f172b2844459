package 예제문제;
import java.util.Scanner;
public class javapro {
	public static void main(String[] args) {
		Scanner scanner=new Scanner(System.in);
		int num=scanner.nextInt();
		switch(num/10) {
		case 10:
		case 9:
			System.out.print("A");
			break;
		case 8:
			System.out.print("B");
			break;
		case 7:
			System.out.print("C");
			break;
		default :
			System.out.print("F");
			
		}
		
	}
	}