package 예제문제;
import java.util.Scanner;

class person{
	private int weight;
	int age;
	protected int height;
	public String name;
	public void setweight(int weight) {
		this.weight=weight;
	}
	public int getweight() {
		return weight;
	}}
class student extends person{
	public void set() {
		age=21;
		height=175;
		name="이제운";
		setweight(75);
	}
	public void show() {
		System.out.print("나이: "+age+"키: "+height+"이름: "+name+"몸무게: "+getweight());
	}
}

public class javapro {
	public static void main(String[] args) {
		student lee=new student();
		lee.set();
	}
		
	}
			
