package 공부하자;
import java.util.Scanner;

class Book{
	String title;
	String author;
	public Book(String t,String a) {
		this.title=t;
		this.author=a;
	}
	void show() {
		System.out.print(title+" "+author);
	}
}




public class javagobu {
	public static void main(String[] args) {
		Scanner scanner=new Scanner(System.in);
		
		Book b[]=new Book[2];
		for(int i=0;i<b.length;i++) {
			b[i].title=scanner.next();
			b[i].author=scanner.next();
		}
		b[0].show();
		b[1].show();
		
	}
}


