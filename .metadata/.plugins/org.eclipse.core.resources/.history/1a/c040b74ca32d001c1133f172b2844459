package 예제문제;
import java.util.Scanner;

class Calc{
	public static int abs(int a) {
		return a>0? a:-a;
	}
	public static int max(int a,int b) {
		return a>b? a:b;
	}
	public static int min(int a,int b) {
		return a>b? b:a;
	}
}
public class javapro {
	public static void main(String[] args) {
		int a=10;int b=50;
		int ab=Calc.abs(a);
		int M=Calc.max(a,b);
		int m=Calc.min(a,b);
		System.out.print(ab);
		System.out.print(M);
		System.out.print(m);
	}}