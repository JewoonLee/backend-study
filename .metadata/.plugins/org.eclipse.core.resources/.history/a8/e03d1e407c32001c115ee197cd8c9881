package 공부하자;
import java.util.Scanner;

public class javagobu {

	public static void main(String[] args) {
		int array[]=new int[5];
		for(int i=0;i<array.length;i++) {
			array[i]=i;
		}
		int max=0;
		for(int j=0;j<array.length;j++) {
			if(max<=array[j]) {
				max=array[j];
			}
		}
		System.out.println(max);
		}
	}



